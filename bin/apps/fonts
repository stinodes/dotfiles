#!/bin/bash

function installed {
    if [ -z "$(fc-list | grep "FiraCode")" ]; then
        return 1
    fi
    return 0
} 

function install {
    sudo pacman -S --noconfirm ttf-firacode-nerd
}

function uninstall {
    sudo pacman -R --noconfirm ttf-firacode-nerd
}

function setup {
    exit 0
}
