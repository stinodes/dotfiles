#!/bin/bash

function installed {
    stn-is-installed tmux
}

function install {
    sudo pacman -S --noconfirm tmux 
}

function uninstall {
    sudo pacman -R --noconfirm tmux
}

function setup {
    target=$HOME/.config/tmux
    stn-backup $target
    ln -nsf $STN_CONFIG_DIR/tmux $HOME/.config/tmux
}
